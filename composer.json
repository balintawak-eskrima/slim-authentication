{
    "name": "balintawak-eskrima/slim-authentication",
    "description": "Simple authentication system based on Slim framework",
    "type": "project",
    "license": "open",
    "authors": [
        {
            "name": "Bogdan Titomir",
            "email": "bogdan.titomir@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.4.1",
        "laminas/laminas-diactoros": "^2.1@dev",
        "league/plates": "^4.0@dev",
        "php-di/php-di": "dev-master",
        "slim/psr7": "dev-master",
        "slim/slim": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1@dev"
    },
    "autoload": {
        "psr-4": {
            "Acme\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
